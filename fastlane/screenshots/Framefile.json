{
  "device_frame_version": "latest",
  "default": {
    "keyword": {
      "font": "./fonts/HiraginoKakuGoBold.ttc",
      "font_size": 96,
      "color": "#E03131"
    },
    "title": {
      "font": "./fonts/HiraginoMaruGo.ttc",
      "font_size": 64,
      "color": "#FFFFFF"
    },
    "background": "./background.png",
