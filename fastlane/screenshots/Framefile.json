    },
    {
      "filter": "en-US",
      "keyword": {
        "font": "./fonts/SFProRounded.ttf",
        "font_size": 96,
        "color": "#E03131"
      },
      "title": {
        "font": "./fonts/SFPro.ttf",
        "font_size": 64,
        "color": "#FFFFFF"
      }
    }
  ]
